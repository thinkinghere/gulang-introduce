//判断奇数
const isOdd = function(n){
    return n % 2 != 0
}

// 判断偶数
const isEven = function(n){
    return n % 2 == 0
}
var n1 = isOdd(4)
log('n1', n1)

const min = function(a, b) {
    if (a < b) {
        return a
    } else {
        return b
    }
}

log('min is:', min(1, 3))
